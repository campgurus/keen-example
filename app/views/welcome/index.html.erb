<script src="https://d26b395fwzu5fz.cloudfront.net/3.2.2/keen.min.js" type="text/javascript"></script>
<script type="text/javascript">
var client = Keen.configure({
  projectId: "<%= ENV['KEEN_PROJECT_ID'] %>",
  readKey: "<%= ENV['KEEN_READ_KEY'] %>"
});

Keen.ready(function(){
// --------------------------------------------------------
// Program Views Column Chart
// --------------------------------------------------------

  var total_pageviews = new Keen.Query("count", {
    eventCollection: "view_program",
    timeframe: "this_30_days",
    interval: "daily",
    groupBy: "program_id"
  });
  client.draw(total_pageviews, document.getElementById("total-views-columnchart"), {
  chartType: "columnchart",
  title: "Daily views (last 30 days)",
  chartOptions: {
    isStacked: true
  }
  });

</script>
